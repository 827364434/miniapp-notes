/* pages/myinfo/myinfo.wxss */
page {
  display: block;
  min-height: 100%;
  background-color: #F1F0EB;
}
view{
  text-align:center;
}
.header_bg{
  width: 100%;
  height: 200px;
}
.header_bg image{
  width: 75px;
  height: 75px;
  border-radius: 100%;
  align-items: center;
  justify-content: center;
  z-index: 11;
  margin-top: 50px;
  overflow: hidden;
}
.foot_bg{
  width: 100%;
  height: auto;
  background-color: #fff;
  margin-bottom: 40rpx;
}
.tm_item{
  flex-direction: row;
  height: 50px;
  display: flex;
  align-items: center;
  width: 100%;
}
.tm_item image{
  width: 25px;
  height: 25px;
  margin-left: 20px;
}
.tm_item text{
  color: #000000;
  size: 32rpx;
  margin-left: 20px;
}
.line{
  width: 100%;
  height: 1px;
  background-color: #F2F2F2;
  margin-left: 20px;
  margin-right: 20px;
}